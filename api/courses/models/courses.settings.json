{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "name": "Courses",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "poster": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "uuid": {
      "type": "uid"
    },
    "content_creator": {
      "via": "courses",
      "model": "content-creators"
    },
    "paid_users": {
      "via": "paid_courses",
      "plugin": "users-permissions",
      "collection": "user",
      "dominant": true
    },
    "videos": {
      "type": "component",
      "repeatable": true,
      "component": "videos.video"
    },
    "short_desc": {
      "type": "string",
      "maxLength": 500
    },
    "enrolled_users": {
      "via": "enrolled_courses",
      "plugin": "users-permissions",
      "collection": "user",
      "dominant": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "rating": {
      "type": "component",
      "repeatable": true,
      "component": "rating.rating"
    },
    "paid_users_detail": {
      "type": "component",
      "repeatable": true,
      "component": "paid-users-detail.paid-users-detail"
    },
    "course_price": {
      "model": "price"
    },
    "announcement": {
      "model": "announcement"
    },
    "long_descx": {
      "type": "richtext"
    }
  }
}
