{
  "kind": "collectionType",
  "collectionName": "courses",
  "info": {
    "name": "Courses",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "poster": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {}
    },
    "uuid": {
      "type": "uid"
    },
    "content_creator": {
      "via": "courses",
      "model": "content-creators"
    },
    "paid_users": {
      "via": "paid_courses",
      "plugin": "users-permissions",
      "collection": "user",
      "dominant": true
    },
    "short_desc": {
      "type": "string",
      "maxLength": 500,
      "required": true
    },
    "enrolled_users": {
      "via": "enrolled_courses",
      "plugin": "users-permissions",
      "collection": "user",
      "dominant": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "rating": {
      "type": "component",
      "repeatable": true,
      "component": "rating.rating"
    },
    "paid_users_detail": {
      "type": "component",
      "repeatable": true,
      "component": "paid-users-detail.paid-users-detail"
    },
    "long_descx": {
      "type": "richtext"
    },
    "features": {
      "type": "component",
      "repeatable": true,
      "component": "features.features"
    },
    "grouped_videos": {
      "model": "grouped-videos",
      "via": "course"
    },
    "announcement": {
      "model": "announcement",
      "via": "course"
    },
    "course_price": {
      "via": "course",
      "model": "price"
    },
    "display_siswa": {
      "type": "boolean",
      "default": true
    }
  }
}
